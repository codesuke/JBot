# Server Configuration
server.port=8080
spring.application.name=ChatBot

# Gemini API Configuration
gemini.api-key=YOUR_GEMINI_API_KEY_HERE
gemini.model=gemini-pro

# Supabase Configuration (Database - Optional)
supabase.url=https://your-project.supabase.co
supabase.key=your-supabase-anon-key
supabase.service-key=your-supabase-service-key

# JWT Configuration
jwt.secret=generate-a-random-secret-with-sufficient-entropy
jwt.expiration=86400000

# CORS Configuration
app.cors.allowedOrigins=http://localhost:3000

# Logging Configuration
logging.level.root=INFO
logging.level.com.chatbot=DEBUG

# Spring Boot Configuration
spring.autoconfigure.exclude=org.springframework.boot.autoconfigure.orm.jpa.HibernateJpaAutoConfiguration,org.springframework.boot.autoconfigure.jdbc.DataSourceAutoConfiguration
